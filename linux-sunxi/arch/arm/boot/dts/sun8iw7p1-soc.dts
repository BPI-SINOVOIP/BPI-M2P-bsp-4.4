/*
 * Allwinner Technology CO., Ltd. sun8iw7p1 soc board.
 *
 */

/dts-v1/;

#include "sun8iw7p1.dtsi"

/{

	soc@01c00000 {
	};
	wlan:wlan {
		compatible = "allwinner,sunxi-wlan";
		wlan_busnum = <1>;
		wlan_usbnum = <3>;
		wlan_power;
		wlan_io_regulator;
		wlan_en;
		wlan_regon;
		wlan_hostwake;
		status = "okay";
	};

};
