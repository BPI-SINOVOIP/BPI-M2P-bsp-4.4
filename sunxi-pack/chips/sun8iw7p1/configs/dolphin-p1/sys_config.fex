ths_trip1_1         = 85
ths_trip1_2         = 90
ths_trip1_3         = 95
ths_trip1_4         = 100
ths_trip1_5         = 0
ths_trip1_6         = 0
ths_trip1_7         = 0
ths_trip1_0_min     = 0
ths_trip1_0_max     = 0
ths_trip1_1_min     = 1
ths_trip1_1_max     = 1
ths_trip1_2_min     = 1
ths_trip1_2_max     = 1
ths_trip1_3_min     = 2
ths_trip1_3_max     = 2
ths_trip1_4_min     = 2
ths_trip1_4_max     = 2
ths_trip1_5_min     = 3
ths_trip1_5_max     = 4
ths_trip1_6_min     = 4
ths_trip1_6_max     = 4
ths_trip2_count     = 1
ths_trip2_0         = 115
